#Hapilin
a =  open("p8.txt", "r")
z = open("p8viv.txt","w")
f = a.readlines()
k = int(0)
for i in f:
    k = int(0)
    for j in i:
        try:
            j = int(j)
            if type(j) == int:
                k += j
        except:
            j = str(j)
    z.write(str(k) + "\n")
z.close()
a.close()

p8.txt
2 2 bghbg
 3    4
 1       5

p8viv.txt
4
7
6
