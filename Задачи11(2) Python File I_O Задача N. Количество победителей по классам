#Hapilin
a = open("2_5.txt","r")
b = open("2_5viv.txt","w")
f = a.readlines()
nin_th = int(0)
ten_th = int(0)
eleven_th = int(0)
counter_ludey = int(0)
chel = str()
count1 = int(0)
count2 = int(0)
count3 = int(0)
for i in f:
    i = i.split()
    if i[2] == "9" and int(nin_th) < int(i[3]):
        nin_th = i[3]
    if i[2] == "10" and int(ten_th) < int(i[3]):
        ten_th = i[3]
    if i[2] == "11" and int(eleven_th) < int(i[3]):
        eleven_th = i[3]
print(eleven_th)
for i in f:
    i = i.split()
    if i[2] == "9" and int(nin_th) == int(i[3]):
        count1 += 1
    if i[2] == "10" and int(ten_th) == int(i[3]):
        count2 += 1
    if i[2] == "11" and int(eleven_th) == int(i[3]):
        count3 += 1

b.write(str(count1) + " " + str(count2) + " " + str(count3))
b.close()
a.close()

